{
  "manifest_version": 2,

  "name": "GUI Tester",
  "description": "Tento doplněk testuje GUI elementy webové stránky",
  "version": "0.3",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "devtools_page": "devtools.html",
    
   "web_accessible_resources" : ["extension-script.js", "jquery.min.js"],
  

  "permissions": [
    "https://*/",
    "file://*/",
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
     ]

}
